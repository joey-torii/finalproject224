    <script type="text/html" id="template_comment">
        <div data-bind="attr: { id: 'Comment' + commentID }" class="comment">
            <div class="comment-user-holder ">
                <div class="comment-user clearfix" data-bind="css: { 'comment-user-reply': (parentCommentID), 'highlight': ($root.highlightedCommentCreated() == created) }">
                    <div class="comment-avatar">
                        <a href="#" data-bind="attr: { href: ('/user/' + mgUserID) }"><img class="mguser-border" data-bind="attr: { src: Mg.getUserImage($data) }"></a>
                    </div><!-- /.comment-avatar -->
                    <div class="comment-cnt">
                        <div class="comment-row-top">
                            <div class="username">
                                <h4><a href="#" data-bind="text: Mg.getCommentUsername($data), attr: { href: ('/user/' + mgUserID) }"></a></h4>
                            </div><!-- /.username -->
                            <!-- /.comment-likes -->
                            <div class="time-submitted">
                                <a data-bind="text: jQuery.timeago(Mg.msToJsDate(created, -8))"></a>
                            </div><!-- /.time-submitted -->
                        </div><!-- /.comment-row-top -->
                        <div class="comment-text">
                            <p data-bind="html: Mg.getCommentHtml($data)"></p>
                        </div><!-- /.comment-text -->
                        <div class="comment-options">
                            <div class="opt opt-reply">
                                <a href="#" data-bind="click: jQuery.proxy($root.replyTo, $root)">Reply</a>
                            </div><!-- /.opt -->
                            <div class="opt opt-delete" data-bind="visible: $root.canDeleteComment($data)">
                                <a href="#" data-bind="click: jQuery.proxy($root.delete, $root)">Delete</a>
                            </div><!-- /.opt -->
                            <div class="opt opt-like" data-bind="with: entityVotesSummary">
                                <div class="voter horizontal" data-bind="attr: { 'data-entity-name': entityName, 'data-entity-id': entityID }" onclick="alertLoginRequired();">

    <a class="downvote" data-bind="css: { 'selected btn-danger': (userVoteScore == -1) }" onmousedown="if (event.button != 0) return; Voter_OnDownVoteClick(this.parentNode);">
    </a>

    <span class="score" data-bind="text: totalVotesSum, css: { upvoted: (userVoteScore == 1), downvoted: (userVoteScore == -1) }">
    </span>

    <a class="upvote" data-bind="css: { 'selected btn-success': (userVoteScore == 1) }" onmousedown="if (event.button != 0) return; Voter_OnUpVoteClick(this.parentNode);">
    </a>
</div>
                            </div>
                            <!-- /.opt -->
                        </div><!-- /.comment-options -->
                    </div><!-- /.comment-cnt -->
                </div><!-- /.comment-user -->

                <div data-bind="template: { name: 'template_comments', data: { all: $parent.all, show: Mg.getCommentReplies($data, $parent.all) } }"></div>
            </div><!-- /.comment-user-holder -->
        </div>
    </script>


